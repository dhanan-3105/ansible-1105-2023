demo.oci.yaml



plugin: oracle.oci.oci

regions:
 - eu-frankfurt-1

compartments:
 - compartment_ocid: "Your-compartment-id"
   fetch_hosts_from_subcompartments: false


---
- hosts: localhost
  become: true
  tasks:
    - name : create file
      file: path=/tmp/testfile state=touch
    - name : print
      debug:
       msg: file created
